/* pages/report/reportRecord.wxss */
.recordCell{
  display: flex;
  flex-direction: column;
  height: 140rpx;
}
.recordCell .info{
  display: flex;
  flex-direction: row;
  margin-left: 40rpx;
}
.recordCell .info .state{
  position: absolute;
  right: 60rpx;
  color: red;
}
.recordCell .title{
  font-size: 40rpx;
  margin-left: 40rpx;
}
.divide{
  height: 1px;
  width: 100%;
  background-color: #F6F6F6;
  margin-top: 20rpx;
}
.things{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 200rpx;
  align-items: center;
}