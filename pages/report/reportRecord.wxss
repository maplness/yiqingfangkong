/* pages/report/reportRecord.wxss */
.recordCell{
  display: flex;
  flex-direction: column;
}
.recordCell .info{
  display: flex;
  flex-direction: row;
}
.recordCell .info .state{
  color: red;
}
.recordCell .title{
  font-size: 40rpx;
}
.recordCell::after{
  content: " ";
  position: absolute;
  height: 5px;
  right: 0;
  bottom: 0;
  left:0;
  background-color: #E9E9E9;
  border: 1rpx solid lightgray;
}
.divide{
  height: 1px;
  width: 100%;
  background-color: #F6F6F6;
}