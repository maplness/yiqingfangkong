/* pages/mainpage/functions.wxss */
page{
    background-color: #F2F6F8;
}
.wrapper{
    height: auto;
    background-color: #ffffff;
}
.l1 .f-l{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 20rpx 32rpx;
}
.l1 .f-l .t{
    font-size:16px;
    font-family:PingFangSC-Regular,PingFang SC;
}
.l1 .f-l .edit{
    font-size:16px;
    font-family:PingFangSC-Medium,PingFang SC;
    color: #1677FF;
}
.function-wrapper{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    width: 100%;
    height: auto;
    align-items: center;
    justify-content: flex-start;
}
.function-cell{
    width: 150rpx;
    height: 126rpx;
    padding-bottom: 40rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.function-cell .cell-icon{
    width: 76rpx;
    height: 76rpx;
}
.function-cell .cell-title{
    opacity: 0.85;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #000000;
    letter-spacing: 0;
    text-align: center;
}
.h-divider{
    width: 100%;
    height: 16rpx;
    background-color: #F2F6F8;
}
.title{
    margin: 32rpx;
    font-size:16px;
    font-family:PingFangSC-Regular,PingFang SC;
}