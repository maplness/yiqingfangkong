/* pages/notification/notification.wxss */
page{
    background-color: #F5F5F5;
}
.top{
    width: 100%;
    height: 24rpx;
}
.n-cell{
    display: flex;
    flex-direction: column;
    padding: 32rpx;
    background-color: #ffffff;
}
.n-cell .title{
  /* height: 104rpx; */
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.n-cell .s-l{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-top: 24rpx;
    font-size: 24rpx;
}
.desc{
    color: #8D8D8D;
}
.h-divider{
    width: 100%;
    height: 1px;
    background-color: #f5f5f5;
}
.noData{
  text-align: center;
  /* margin-top: 10rpx; */
}
